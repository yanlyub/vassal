<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox
        xmlns="http://javafx.com/javafx/10.0.2-internal"
        xmlns:fx="http://javafx.com/fxml/1"
        alignment="CENTER"
        minHeight="300.0"
        minWidth="300.0"
        fx:controller="VASSAL.launch.playerwizard.WizardMainController">
    
    <Button fx:id="newGameButton" contentDisplay="CENTER" onAction="#newGameButtonClicked" text="New Game" />
    <Button text="Load Game" />
    <Button text="Play Online" />

</VBox>
